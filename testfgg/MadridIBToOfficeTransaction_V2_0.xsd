		<xsd:annotation>
			<xsd:documentation>Date of subsequent designation of the previous registration</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PreviousRegistrationPriorityDate" type="com:DateType">
		<xsd:annotation>
			<xsd:documentation>Priority date of the previous registration</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PreviousRegistrationCancellationDate" type="com:DateType">
