	</xsd:element>
	<xsd:element name="PreviousRegistrationDate" type="com:DateType">
		<xsd:annotation>
			<xsd:documentation>Registration date of previous mark</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PreviousRegistrationNumber" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Registration number of previous mark</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NationalDesignSearchCode" type="tmk:NationalDesignSearchCodeType">
