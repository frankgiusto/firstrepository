				<xsd:element ref="tmk:MarkImageColourPartClaimedText" minOccurs="0" maxOccurs="3"/> <!-- changed -->
				<xsd:element ref="tmk:MarkDescriptionText" minOccurs="0" maxOccurs="3"/> <!-- changed -->
				<xsd:element ref="tmk:MarkTranslationTextBag" minOccurs="0"/> 
				<xsd:element ref="tmk:MarkDisclaimerBag" minOccurs="0"/>
				<xsd:element ref="tmk:GoodsServicesBag"/>
				<xsd:element ref="tmk:GoodsServicesLimitationBag" minOccurs="0"/>
				<xsd:element ref="tmk:BasicRegistrationApplicationBag" minOccurs="0"/>
				<xsd:element ref="tmk:PriorityBag" minOccurs="0"/>
				<xsd:element ref="tmk:SeniorityBag" minOccurs="0"/>
				<xsd:element ref="tmk:DesignatedCountryBag"/>
				<xsd:element ref="tmk:RecordUseIntentBag" minOccurs="0"/>
				<xsd:element ref="tmk:BasicRecordBag" minOccurs="0"/>
<!--				<xsd:element ref="tmk:GoodsServicesLimitationBag" minOccurs="0"/>  duplicate?-->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="MadridIBTransactionCountBag" type="tmk:MadridIBTransactionCountBagType">
		<xsd:annotation>
			<xsd:documentation>A collection of Madrid IB to office transaction count</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
