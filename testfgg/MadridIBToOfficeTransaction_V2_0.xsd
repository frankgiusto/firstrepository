			<xsd:documentation>Collection of the mark Holders</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MadridDesignationCategory" type="tmk:MadridDesignationCategoryType">
		<xsd:annotation>
			<xsd:documentation>MadridDesignationCategoryType</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MarkTransliteration" type="tmk:MarkTransliterationType">
