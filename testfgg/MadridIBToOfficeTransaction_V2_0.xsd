		<xsd:annotation>
			<xsd:documentation>Collection of basic applications in the office of origin</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ClassDescriptionBag" type="tmk:ClassDescriptionBagType">
		<xsd:annotation>
			<xsd:documentation>Collection of Goods and Services classes and descriptions</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
