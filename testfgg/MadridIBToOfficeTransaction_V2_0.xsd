					<xsd:documentation>Holder representative change</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Protection restriction">
				<xsd:annotation>
					<xsd:documentation>Protection restriction</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Completed processing">
				<xsd:annotation>
					<xsd:documentation>Completed processing</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="International registration renewal">
				<xsd:annotation>
					<xsd:documentation>International registration renewal</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Record licence">
				<xsd:annotation>
					<xsd:documentation>Record licence</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Licensee record change">
				<xsd:annotation>
					<xsd:documentation>Licensee record change</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Second part payment">
				<xsd:annotation>
					<xsd:documentation>Second part payment</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
