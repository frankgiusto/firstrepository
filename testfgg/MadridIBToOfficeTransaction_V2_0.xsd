	<xsd:element name="PriorityRegistrationRenewalExpiredDate" type="com:DateType">
		<xsd:annotation>
			<xsd:documentation>The renewal expiration date of the foreign priority registration. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PriorityRegistrationRenewedDate" type="com:DateType">
