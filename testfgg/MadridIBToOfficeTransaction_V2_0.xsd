			<xsd:documentation>The renewal date of the foreign priority registration.   </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PriorityRegistrationExpiredDate" type="com:DateType">
		<xsd:annotation>
			<xsd:documentation>The expiration date of the foreign priority registration.  </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PriorityRegistrationDate" type="com:DateType">
		<xsd:annotation>
			<xsd:documentation>The registration date of the foreign priority registration.  </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PriorityRegistrationNumber" type="xsd:string">
